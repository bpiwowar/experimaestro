# Editor configuraiton file
# See http://EditorConfig.org for more information

# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true

# Set default charset
[*.{js,py}]
charset = utf-8

# 3 space indentation
[*.{py,i}]
indent_style = space
indent_size = 3
